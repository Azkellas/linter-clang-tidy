{
    "name": "linter-clang-tidy",
    "main": "./lib/linter-clang-tidy",
    "version": "0.0.0",
    "description": "Linter for clang-tidy on atom",
    "keywords": [
    ],
    "repository": "https://github.com/atom/linter-clang-tidy",
    "license": "MIT",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "dependencies": {
    },
    "providedServices": {
        "linter": {
            "versions": {
                "2.0.0": "provideLinter"
            }
        }
    },
    "configSchema": {
        "execPath": {
            "type": "string",
            "default": "clang-tidy",
            "description": "Command or full path to clang-tidy",
            "order": 1
        },
        "options": {
            "type": "string",
            "default": "-filter-header=./ -checks=*",
            "description": "Options for clang-tidy",
            "order": 2
        }
    }
}
